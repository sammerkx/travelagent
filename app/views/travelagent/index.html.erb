<h1>Travelagent dot com</h1>
<p>Find us at the Meston Building</p>

 <div id="container" style="width:100%; height:400px;"></div>
 
 <script type="text/javascript">
$(function () {
    var myChart = Highcharts.chart('container', {
        chart: {
            type: 'bar'
        },
        title: {
            text: 'Cruise Bookings'
        },
        xAxis: {
            categories: <%=raw @cruise_names %>
        },
        yAxis: {
            title: {
                text: 'Cabins Available'
            },  tickInterval: 1
        },
        series: [{
            name: 'Cabins Available',
            data: <%=raw @cabins_available %>
        }, {
            name: 'Cruises',
            data: <%= raw @cruise_names %>
        }]
    });
});
</script>